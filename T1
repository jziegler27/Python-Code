#This is a test file for py_p3.py  
#!/bin/sh

cat > tempin <<EOF
    YMOVE 10              # along y-axis
    SET VARMOVE 0         # sets VARMOVE to 0
L1  ADD VARMOVE 1         # adds 1 to VARMOVE
    xmove 1               # along x-axis
    IF varmove < 8 L1
    YMOVE -3
    PRINTLOC
    HALT
EOF

python3.7 ./$1 tempin 

echo
echo "RMB: 8,7"

rm -f tempin
